{
    "env": {
        "es6": true
    },
    "extends": ["standard", "plugin:prettier/recommended"],
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": ["prettier"],
    "rules": {
        "dot-notation": "off",
        "no-lone-blocks": "off",
        "no-new-func": "off",
        "no-undef": "off",
        "no-unused-vars": "warn",
        "prettier/prettier": "error",
        "quotes": ["error", "double"]
    }
}
